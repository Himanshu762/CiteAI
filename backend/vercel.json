{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py",
      "headers": {
        "Access-Control-Allow-Origin": "https://cite-ai.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept"
      }
    },
    {
      "src": "/(.*)",
      "methods": ["OPTIONS"],
      "dest": "main.py"
    }
  ]
} 